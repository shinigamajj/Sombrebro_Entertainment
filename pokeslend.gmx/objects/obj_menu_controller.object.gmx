        instance_create(5,165,menu_button_yes);
        instance_create(5,5,menu_sure);
        menu_button_yes.isSelected = true;
    } else { exit; }
} else if (menuState == 2) {
    if(menu_button_back.isSelected &amp;&amp; keyboard_check_pressed(vk_enter)){
