        randNum = irandom_range(1,3);
        if(randNum == 1) { audio_play_sound(sound_BG_forest_01,0,0); }
            else if(randNum == 2) { audio_play_sound(sound_BG_forest_02,0,0); }
            else if(randNum == 3) { audio_play_sound(sound_BG_forest_03,0,0); }
        room_goto(tutorial_room_01);
    } else { exit; }
} else if (menuState == 3) {
    if(menu_button_back.isSelected &amp;&amp; keyboard_check_pressed(vk_enter)){
