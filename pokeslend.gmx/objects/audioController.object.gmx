            if(audio_is_playing(sound_char_walk_swamp_deep)){
                    exit;
            } else { audio_play_sound(sound_char_walk_swamp_deep,0,0); }   
        } else { /* Run in Swamp */
            if(audio_is_playing(sound_char_sprint_swamp_deep)){
                    exit;
