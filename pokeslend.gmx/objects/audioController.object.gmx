            if(room_caption == "tutorial_room_01" || room_caption == "forest_room_02"){
                if(audio_is_playing(sound_char_sprint_tall_grass)){
                    exit;
                } else { audio_play_sound(sound_char_sprint_tall_grass,0,0); }
